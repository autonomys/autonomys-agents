{
  "name": "autonomys-agents",
  "version": "0.4.1",
  "description": "Autonomys Agents",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/autonomys/autonomys-agents"
  },
  "author": {
    "name": "Autonomys",
    "url": "https://www.autonomys.xyz"
  },
  "bugs": {
    "url": "https://github.com/autonomys/autonomys-agents/issues"
  },
  "engines": {
    "node": ">=20.18.1"
  },
  "scripts": {
    "build": "tsc",
    "start": "tsx scripts/start.ts",
    "dev": "NODE_ENV=development tsx --no-cache --watch src/index.ts",
    "dev:all": "tsx scripts/dev-all.ts",
    "example:twitter-agent": "tsx examples/twitterAgent/index.ts",
    "example:multi-personality": "tsx examples/multiPersonality/index.ts",
    "example:web3-agent": "tsx examples/web3Agent/index.ts",
    "example:vector-db": "tsx examples/vectorDbExample.ts",
    "example:slack-agent": "tsx examples/slackAgent/index.ts",
    "example:github-agent": "tsx examples/githubAgent/index.ts",
    "example:notion-agent": "tsx examples/notionAgent/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\" \"agent-package-manager/**/*.ts\" \"web-cli/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "generate-certs": "tsx scripts/generate-certs.ts",
    "resurrect": "tsx scripts/resurrect.ts",
    "create-character": "tsx scripts/create-character.ts",
    "migrate:vector-db": "tsx scripts/migrate-vector-db.ts"
  },
  "dependencies": {
    "@autonomys/auto-dag-data": "1.4.5",
    "@autonomys/auto-drive": "1.4.5",
    "@autonomys/auto-utils": "1.4.5",
    "@langchain/anthropic": "0.3.15",
    "@langchain/community": "0.3.36",
    "@langchain/core": "0.3.42",
    "@langchain/groq": "0.2.0",
    "@langchain/langgraph": "0.2.57",
    "@langchain/ollama": "0.2.0",
    "@langchain/openai": "0.5.0",
    "@notionhq/client": "^2.2.16",
    "@octokit/rest": "^21.1.1",
    "@slack/web-api": "^7.8.0",
    "@types/helmet": "^4.0.0",
    "@types/xml2js": "^0.4.14",
    "agent-twitter-client": "0.0.18",
    "axios": "^1.7.9",
    "better-sqlite3": "^11.8.0",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.13.4",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "helmet": "^8.0.0",
    "http2-express-bridge": "^1.0.7",
    "inquirer": "^10.2.0",
    "sqlite3": "^5.1.7",
    "vectorlite": "^0.2.0",
    "winston": "^3.11.0",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "22.10.0",
    "@types/sqlite3": "^3.1.11",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.1",
    "glob": "^10.3.10",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "memfs": "^4.7.7",
    "prettier": "^3.2.2",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.20.0"
  }
}
